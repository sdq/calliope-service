{
  "name": "activity-server",
  "version": "1.0.0",
  "description": "activity server",
  "private": true,
  "egg": {
    "declarations": true
  },
  "dependencies": {
    "egg": "^2.15.1",
    "egg-cors": "^2.2.3",
    "egg-validate": "^2.0.2",
    "egg-mongo-native": "^3.5.0",
    "egg-scripts": "^2.11.0"
  },
  "devDependencies": {
    "acorn": "^7.1.1",
    "autod": "^3.0.1",
    "autod-egg": "^1.1.0",
    "egg-bin": "^4.11.0",
    "egg-ci": "^1.11.0",
    "egg-mock": "^3.21.0",
    "eslint": "^5.13.0",
    "eslint-config-egg": "^7.1.0",
    "mocha": "^5.2.0"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "scripts": {
    "start": "egg-scripts start --title=activity-server --port=7002 --workers=16",
